    <p>
      <%= link_to_unless_current(
        'blog_posts', :controller => 'admin/blog_posts'
      ) %>
      | <%= link_to_unless_current(
        'blog_posts2', :controller => 'admin/blog_posts2'
      ) %>
      | <%= link_to_unless_current(
